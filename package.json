{
  "name": "openai",
  "version": "1.1.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.json && cp .env ./build/.env && cp google-credentials.json ./build/google-credentials.json",
    "start": "node build/index.js",
    "dev": "ts-node-dev --respawn --transpile-only --quiet src/index.ts",
    "format": "prettier --write \"src/\"",
    "lint": "eslint src/**/*.ts --fix",
    "type": "tsc --project tsconfig.json --noEmit"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@google-cloud/speech": "^6.5.0",
    "discord.js": "^14.11.0",
    "dotenv": "^16.0.3",
    "fluent-ffmpeg": "^2.1.2",
    "joi": "^17.9.2",
    "node-telegram-bot-api": "^0.61.0",
    "openai": "^3.2.1",
    "pg": "^8.11.0",
    "typeorm": "^0.3.16"
  },
  "devDependencies": {
    "@gazzati/eslint-config-node": "^1.0.7",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^20.2.3",
    "@types/node-telegram-bot-api": "^0.61.6",
    "eslint": "^8.41.0",
    "module-alias": "^2.2.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  }
}
